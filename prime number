import java.util.*;
public class Prime
{
    public static void main(String args[])
    {
        int i,n,c=0;
        Scanner in = new Scanner(System.in);
        n=in.nextInt();
        for(i=1;i<=n;i++)
        {
            if(n%i==0)
            {
                c++;
            }
        }
        if(c==2)
        System.out.println(n+" "+"PRIME");
        else
        System.out.println(n+" "+"NOT PRIME");
    }
}
